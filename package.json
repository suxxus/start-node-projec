{
  "name": "node-project-starter",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "suxxus",
  "license": "ISC",
  "description": "node project starter, lint-tape ",
  "scripts": {
    "lint": "eslint --color  src || true",
    "test": "NODE_PATH=src tape src/tests | tap-colorize",
    "watch:test": "npm test & npm run lint",
    "watch": "watch -p 'src/**/*.js' -c 'npm run watch:test'",
    "dev": "npm run watch",
    "check": "npm outdated --depth=0",
    "update": "updtr"
  },
  "devDependencies": {
    "eslint": "2.7.0",
    "eslint-cli": "1.1.0",
    "eslint-config-eslint": "3.0.0",
    "tap-colorize": "1.2.0",
    "tape": "4.5.1",
    "updtr": "0.1.10",
    "watch-cli": "0.2.1"
  },
  "keywords": [
    "node",
    "project"
  ]
}
